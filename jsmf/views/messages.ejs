<% if (typeof errors!='undefined'){ %>
    <% for( var i=0; i<errors.length;i++){ %>
<div class="alert alert-danger" role="alert">
    <%= errors[i].msg %>
</div>

    <% } %>
<% }%>


<% if(success_msg !=''){ %>
    <div class="alert alert-success" role="alert">
            <%= success_msg %>
          </div>
<%} %>
<% if(error_msg!=''){ %>
  <div class="alert alert-danger" role="alert">
          <%= error_msg %>
          
        </div>
<%} %>
<% if(error !=''){ %>
  <div class="alert alert-danger" role="alert">
          <%= error %>
        </div>
<%} %>


